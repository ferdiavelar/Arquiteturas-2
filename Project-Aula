///////////////////////////////////////////////////////////////////////////////////////////////////////////
def readCommand(fileName):
	file = open(fileName,"r") #open the file
	command = file.read() # reads it
	file.close() #closes it
	return command # return the command

def  readOperation(command):
	operation = command[0:2] # gets the operation inside de command
	#converts it
	if operation =="00":
		operation ="+"
	elif operation == "01":
		operation ="-"
	elif operation == "10":
		operation ="*"
	elif operation == "11":
		operation ="/"

	return operation # returns the operation

def readRegister(command, a , b):
	register = command[a:b] # gets the register inside de command
	return register #returns the register

def getNumber(register):
	file = open("registrators.txt","r") #open the file
	for line in file: # reads it line by line
		if line[0:6] == register: #if the registrator is found
			number = line[7:10] # gets the number related to it
			return int(number) # return the number
	print("Registrador inv√°lido")
